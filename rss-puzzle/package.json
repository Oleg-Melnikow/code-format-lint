{
  "name": "rss-puzzle",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --config ./webpack.config.js",
    "build": "webpack --watch",
    "format": "npx prettier --write ./**/*.{js,ts,css,scss,html}",
    "ci:format": "npx prettier --check ./**/*.{js,ts,css,scss,html}",
    "lint": "eslint . --ext .ts --fix --max-warnings 0",
    "prepare": "husky",
    "precommit": "npm run format && npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint",
      "pre-push": "npm run format && npm run lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "prettier": "3.2.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  }
}
